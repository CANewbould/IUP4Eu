-- Example 3

include std/types.e -- for string type
include iup.ew

IupOpen()
string name = "*.txt"
object res = IupGetFile(name)
name = res[2]

integer err = res[1]
switch err do
    case 1 then
      IupMessage("New file", name)
    case 0 then
      IupMessage("File already exists", name)
    case -1 then
      IupMessage("IupGetFile", "Operation cancelled")
end switch
IupClose()
