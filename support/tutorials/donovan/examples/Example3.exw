-- Example 3

include std/types.e
include iup.ew

IupOpen()
string name = "*.txt"
sequence res = IupGetFile(name)
name = res[2]

integer err = res[1]
if err = 1 then
  IupMessage("New file", name)
elsif err = 0 then
  IupMessage("File already exists", name)
elsif err = -1 then
  IupMessage("IupGetFile", "Operation cancelled")
end if
IupClose()
